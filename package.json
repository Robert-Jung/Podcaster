  },
  "homepage": "https://github.com/Robert-Jung/Podcaster#readme",
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "nodemon": "^1.11.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "express": "^4.15.2",
    "level": "^1.6.0",
    "material-ui": "^0.18.0",
